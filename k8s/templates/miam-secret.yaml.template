apiVersion: v1
kind: Secret
metadata:
  name: miam-secret
  namespace: miam
  labels:
    app: miam
type: Opaque
data:
  # These should be base64 encoded values, not plaintext
  SUPABASE_URL: "${SUPABASE_URL_B64}"
  SUPABASE_KEY: "${SUPABASE_KEY_B64}"
  SUPABASE_DB_PASSWORD: "${SUPABASE_DB_PASSWORD_B64}"
  OPENAI_API_KEY: "${OPENAI_API_KEY_B64}"
